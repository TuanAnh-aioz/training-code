{
    "task_type": "xxx",
    "model_name": "xxx",
    "dataset": {
        "csv_file": "./metadata.csv",
        "batch_size": 8,
        "num_workers": 4,
        "val_ratio": 0.2,
        "transforms": {
            "train": {
                "resize": 224,
                "normalize": {
                    "mean": [0.485, 0.456, 0.406],
                    "std": [0.229, 0.224, 0.225]
                }
            },
            "val": {
                "resize": 224,
                "normalize": {
                    "mean": [0.485, 0.456, 0.406],
                    "std": [0.229, 0.224, 0.225]
                }
            }
        }
    },
    "epochs": 50,
    "optimizer": {
        "type": "SGD",
        "lr": 0.0005,
        "params": { 
            "momentum": 0.9, 
            "weight_decay": 5e-4, 
            "nesterov": true 
        } 
    },
    "scheduler": {
        "type": "multisteplr",
        "params": {
            "step_size": 10,
            "gamma": 0.1
        }
    },
    "log_interval": 20
}